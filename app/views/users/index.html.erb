 <div id="indexcontainer"> 
  <h1>List of Users</h1>

  <table>
  <tr>
    <th>Users</th>

  </tr>
  <%= @user.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= button_to "X", { action: "destroy", id: user.id}, {method: :delete, data:{confirm:"Are you damned sure???"}, :class => "mybutton"}
      %></td>
    </tr>
  <% end %>
</table>
</div>